<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cenario32"
    tools:context=".screen1_startlogin">

    <Button
        android:id="@+id/buttonStart"
        style="@style/TextAppearance.Material3.BodyMedium"
        android:layout_width="348dp"
        android:layout_height="82dp"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="121dp"
        android:onClick="onClick"
        android:text="COMEÃ‡AR"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textStyle="bold"
        app:backgroundTint="#04B50B"
        app:layout_constraintBottom_toTopOf="@+id/barrier8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top"
        app:layout_constraintBottom_toTopOf="@+id/barrier9"
        app:layout_constraintTop_toBottomOf="@+id/buttonStart" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top"
        app:layout_constraintBottom_toTopOf="@+id/barrier10"
        app:layout_constraintTop_toBottomOf="@+id/barrier8" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top"
        app:layout_constraintBottom_toTopOf="@+id/barrier11"
        app:layout_constraintTop_toBottomOf="@+id/barrier9" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/barrier10" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="440dp"
        android:layout_height="0dp"
        android:layout_marginTop="153dp"
        android:layout_marginBottom="45dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonStart"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logotransparente"
        tools:layout_editor_absoluteX="-15dp" />

</androidx.constraintlayout.widget.ConstraintLayout>